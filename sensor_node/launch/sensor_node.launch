<launch>
  <node name="sensor_main" pkg="sensor_node" type="sensor_main" output="screen">
      <param name="ANGLE_P"    type="double" value="0.95"/>
      <param name="ANGLE_I"    type="double" value="0.3"/>
      <param name="ANGLE_D"    type="double" value="0"/>
      <param name="CUT_OFF_RATIO"    type="double" value="3"/>
      <param name="ROBOT_SPEED"   type="double" value="0.9"/>
      <param name="MIN_ANGLE"    type="int" value="10"/>
      <param name="MAX_DIS"   type="double" value="1.4"/>
      <param name="SIDE_DIS"   type="double" value="0.55"/>
      <param name="MAX_ANGLE"    type="int" value="75"/>
      <param name="MAX_ANGLE_AVOID"   type="int" value="67"/>
      <param name="DECEL"   type="double" value="0.2"/>
  </node>
  
</launch>
